<% if @created -%>
    $("#all_indices").prepend("<%= escape_javascript(render :partial => 'index') %>");
    $("#index_<%= @index.id %>").effect('highlight',{ startcolor:'#ffff00'},8000);
<% else -%>
    $("#updater").html("<%= escape_javascript(render :partial => "share/error") %>");
    $("#updater").effect('slide', {});
<% end -%>


